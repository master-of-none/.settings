{
    "code-runner.runInTerminal": true,
    "security.workspace.trust.untrustedFiles": "open",
    "latex-workshop.linting.chktex.exec.path": "/Library/TeX/texbin/chktex",
    "debug.allowBreakpointsEverywhere": true,
    "git.autofetch": true,
    "git.ignoreRebaseWarning": true,
    "editor.lineNumbers": "relative",
    "git.confirmSync": false,
    "explorer.confirmDragAndDrop": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.CustomBrowser": "chrome",
    "code-runner.executorMap": {
        "javascript": "node",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "cpp": "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "php",
        "python": "python -u",
        "perl": "perl",
        "perl6": "perl6",
        "ruby": "ruby",
        "go": "go run",
        "lua": "lua",
        "groovy": "groovy",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "bat": "cmd /c",
        "shellscript": "bash",
        "fsharp": "fsi",
        "csharp": "scriptcs",
        "vbscript": "cscript //Nologo",
        "typescript": "ts-node",
        "coffeescript": "coffee",
        "scala": "scala",
        "swift": "swift",
        "julia": "julia",
        "crystal": "crystal",
        "ocaml": "ocaml",
        "r": "Rscript",
        "applescript": "osascript",
        "clojure": "lein exec",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "racket": "racket",
        "scheme": "csi -script",
        "ahk": "autohotkey",
        "autoit": "autoit3",
        "dart": "dart",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "haskell": "runhaskell",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "lisp": "sbcl --script",
        "kit": "kitc --run",
        "v": "v run",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "sml": "cd $dir && sml $fileName"
    },
    "explorer.confirmDelete": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "workbench.iconTheme": "material-icon-theme",
    // "[python]": {
    //     "editor.formatOnType": true,
    //     "editor.defaultFormatter": "ms-python.autopep8"
    // },
    "editor.formatOnSave": true,
    "gitlens.hovers.currentLine.over": "line",
    "tabnine.experimentalAutoImports": true,
    "vsintellicode.features.python.deepLearning": "enabled",
    "git.openRepositoryInParentFolders": "never",
    "terminal.integrated.shellIntegration.suggestEnabled": true,
    "C_Cpp.intelliSenseEngineFallback": "enabled",
    "typescript.experimental.tsserver.web.enableProjectWideIntellisense": true,
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "python": false,
        "html": false,
        "javascript": true,
        "rust": false,
        "typescriptreact": false,
        "typescript": false,
        "java": false,
        "go": false,
        "ocaml": false,
        "haskell": false,
        "ocaml.interface": false,
        "dockerfile": false,
        "sql": false,
        "clojure": false,
        "c": false
    },
    "leetcode.workspaceFolder": "/Users/shrikrishnabhat/Desktop/Programming/leetcode-unlocked",
    "leetcode.defaultLanguage": "python3",
    "leetcode.hint.commandShortcut": false,
    "leetcode.hint.configWebviewMarkdown": false,
    "leetcode.hint.commentDescription": false,
    "eslint.run": "onSave",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "prettier.tabWidth": 4,

    "tailwindCSS.includeLanguages": {
        "html": "html",
        "javascript": "javascript",
        "css": "css"
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [],
        "editor.selectionHighlight": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },

    "gopls": {
        "experimentalWorkspaceModule": true
    },
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "[go]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "golang.go",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always"
        }
    },

    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    },
    "[ocaml]": {
        "editor.tabSize": 2
    },
    "[ocamllex]": {
        "editor.tabSize": 4
    },
    "[menhir]": {
        "editor.tabSize": 4
    },
    "[c]": {
        "editor.tabSize": 4
    },
    "editor.rulers": [],
    "redhat.telemetry.enabled": true,
    "githubPullRequests.createOnPublishBranch": "never",
    "githubPullRequests.fileListLayout": "tree",
    // "editor.fontLigatures": "'ss01','ss05','dlig'",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    // Change the font family to your desired font
    // "editor.fontFamily": "'Monaspace Argon', monospace",
    // "editor.fontFamily": "'Victor Mono', 'Fira Code', 'Courier New', monospace",
    "editor.fontFamily": "'Cascadia Code', 'Fira Code', 'Courier New', monospace",
    // "editor.fontFamily": "'Recursive Mono Casual Static', 'Fira Code', 'Courier New', monospace",
    "editor.fontSize": 16,
    "gitlens.graph.minimap.enabled": false,
    "haskell.formattingProvider": "fourmolu",
    "haskell.serverEnvironment": {
        "PATH": "${HOME}/.ghcup/bin:${PATH}"
    },

